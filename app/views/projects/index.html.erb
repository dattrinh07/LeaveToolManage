<div class="row">

	<div class="col-lg-12">
		<h1 class="page-header">Listing Projects</h1>

	</div>

</div>
<div class="row">
	<div class="col-lg-12">
		<div class="panel panel-default">
			<div class="dataTable_wrapper">
				<table class="table table-striped table-bordered table-hover">
					<thead>
						<tr>
							<th>Title</th>
							<th>Description</th>
							<th>Project Manage</th>
							<th colspan="3"></th>
						</tr>
					</thead>
					<tbody>
						<% @listproject.each do |obj| %>
						<tr>
							<td><%= obj.title %></td>
							<td><%= obj.description %></td>
							<td></td>
							<td><%= link_to 'Show', project_path(obj) %></td>
							<td><%= link_to 'Edit',  edit_project_path(obj) %></td>
							<td><%= link_to 'Destroy', project_path(obj), method: :delete, data: { confirm: 'Are you sure?' } %></td>
						</tr>
						<% end %>
					</tbody>
				</table>
			</div>
			<!-- /.panel-body -->
		</div>
		<!-- /.panel -->
	</div>
</div>



